<div class="card-header card-header-primary">
    <div class="row">
        <div class="col-md-6" style="text-align:start"><h2>Relatorio de Faturamento</h2></div>
        <div class="col-md-6" style="text-align:end">
            <button class="btn btn-secondary" disabled>Exportar</button>
            <button class="btn btn-secondary" id="VisualizarFaturamento" onclick="">Visualizar</button>
        </div>
    </div>
</div>
<div class="card-body">
    <table id="tabRelatorioFaturamento" class="table table-hover table-striped" style="text-align:center">
        <tr>
            <th>
               
                CC
            </th>
            <th>
               
                Setor
            </th>
            <th>
                
                Serie
            </th>
            <th>
               
                Nome
            </th>
            <th>
                
                IP
            </th>
           
            <th>
               
                Inicio<br />Total
            </th>
           
            <th>
               
                Final<br />Total
            </th>
            <th>
                
                Modelo
            </th>
            <th>
                
                Franquia
            </th>
            <th>
                
                Valor
            </th>
            <th>
               
                Volume
            </th>
            <th>
               
                VolTotal
            </th>
            <th></th>
        </tr>

        @{
            Site.Models.ModeldnaPrint db = new Site.Models.ModeldnaPrint();
            foreach (var item in db.vw_bilhetagemAtual.ToList())
            {
                <tr>

                    <td>

                        @item.CC
                    </td>
                    <td>

                        @item.setor
                    </td>
                    <td>

                        @item.serie
                    </td>
                    <td>

                        @item.nome
                    </td>
                    <td>

                        @item.ip
                    </td>
                    <td>

                        @item.Initotal
                    </td>
                    <td>

                        @item.Fintotal
                    </td>
                    <td>

                        @item.Modelo
                    </td>
                    <td>

                        @item.franquia
                    </td>
                    <td>

                        @item.valor
                    </td>
                    <td>

                        @item.Volume
                    </td>
                    <td>

                        @item.VolumeTotal
                    </td>

                </tr>
            }

        }

    </table>
    <div class="row">
        <div class="col-md-12" style="text-align:center">
            <div class="btn-group" role="group" aria-label="Basic example">
                <button id="Anterior" type="button" class="btn btn-secondary" onclick="AnteriorPagina('tabRelatorioFaturamento')">Anterior</button>
                <button type="text" class="npag btn-secondary" disabled>1</button>
                <button id="Proxima" type="button" class="btn btn-secondary" onclick="ProximaPagina('tabRelatorioFaturamento')">Proxima</button>
            </div>
        </div>
    </div>
</div>